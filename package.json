{
  "name": "ui-chat",
  "version": "1.0.0",
  "description": "An natural language interface for frontend development",
  "main": "index.js",
  "scripts": {
    "start": "npm run build:server && node dist/server/src/app.js",
    "build:client": "cd client && npm run build",
    "build:server": "tsc && ncp server/src/codestate dist/server/src/codestate",
    "build": "npm run build:client && npm run build:server",
    "dev:client": "cd client && npm start",
    "dev:server": "cd server && npm run dev",
    "dev": "concurrently \"npm run dev:client\" \"npm run dev:server\"",
    "lint": "eslint '*/**/*.{ts,tsx}' --quiet --fix",
    "format": "prettier --write '*/**/*.{ts,tsx}'",
    "test": "jest",
    "test:server": "jest server"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/aditya-pethe/ui-chat.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/aditya-pethe/ui-chat/issues"
  },
  "homepage": "https://github.com/aditya-pethe/ui-chat#readme",
  "devDependencies": {
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.1",
    "@typescript-eslint/eslint-plugin": "^5.59.6",
    "@typescript-eslint/parser": "^5.59.6",
    "eslint": "^8.40.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^4.2.1",
    "jest": "^29.5.0",
    "ncp": "^2.0.0",
    "prettier": "^2.8.8",
    "ts-jest": "^29.1.0"
  },
  "dependencies": {
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "fs": "^0.0.1-security",
    "gpt3-tokenizer": "^1.1.5",
    "langchain": "^0.0.81",
    "openai": "^3.2.1"
  }
}
